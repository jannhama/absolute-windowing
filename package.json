{
  "name": "absolute-windowing",
  "version": "0.2.0",
  "description": "Vue 3 windowing framework",
  "license": "MIT",
  "keywords": [
    "windowing",
    "window manager",
    "javascript",
    "typescript",
    "vue",
    "vue 3",
    "front-end"
  ],
  "repository": "https://github.com/jannhama/absolute-windowing",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/absolute-windowing.css"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "build-types": "vue-tsc -p tsconfig.build.json",
    "build-only": "vite build",
    "build": "npm run type-check && npm run build-only && npm run build-types && npm run copy-styles",
    "test": "npm run build-only && node --test tests/**/*.test.mjs",
    "copy-styles": "node scripts/copyStyles.mjs",
    "lint": "eslint . --fix --cache"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "eslint": "^10.0.0",
    "eslint-plugin-vue": "~10.8.0",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
}